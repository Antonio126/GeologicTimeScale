# baseURI: http://resource.geosciml.org/ontology/timescale/mappings
# imports: http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal
# imports: http://resource.geosciml.org/ontology/timescale/gts-30
# imports: http://www.w3.org/2006/time
# imports: http://www.w3.org/2006/timezone

@prefix mappings:  <http://resource.geosciml.org/ontology/timescale/mappings#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#DateAndTime>
      owl:equivalentClass <http://www.w3.org/2006/time#DateTimeDescription> .

<http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#Instant>
      owl:equivalentClass <http://www.w3.org/2006/time#Instant> .

<http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#Object>
      owl:equivalentClass <http://www.w3.org/2006/time#TemporalEntity> .

<http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#Period>
      owl:equivalentClass <http://www.w3.org/2006/time#Interval> .

<http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#begin>
      owl:equivalentProperty
              <http://www.w3.org/2006/time#hasBeginning> .

<http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#end>
      owl:equivalentProperty
              <http://www.w3.org/2006/time#hasEnd> .

<http://resource.geosciml.org/ontology/timescale/mappings>
      rdf:type owl:Ontology ;
      owl:imports <http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal> , <http://www.w3.org/2006/time> , <http://resource.geosciml.org/ontology/timescale/gts-30> , <http://www.w3.org/2006/timezone> ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string .

<http://www.w3.org/2006/time#timeZone>
      rdfs:subPropertyOf <http://def.seegrid.csiro.au/isotc211/iso19108/2002/temporal#frame> .
